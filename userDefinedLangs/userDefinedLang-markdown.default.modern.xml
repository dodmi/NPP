<!--

What is this?
This is a definition file for custom syntax highlighting of Markdown files in Notepad++
- It is based on Solarized by Ethan Schoonover ( http://ethanschoonover.com/solarized ), which defines the color palette used for highlighting.
- It will fit best with my Solarized-light-blackened theme, but any light theme should be fine 
- It is based on the Markdown definition file delivered with Notepad++

How to use this file?
This file needs to be copied to the userDefinedLangs folder in your Notepad++ config folder ( %AppData%\Notepad++\userDefinedLangs for default installations )
Be sure to replace the existing file coming with Notepad++
(Re)start Notepad++ after copying the file and you should get a Solarized syntax highlighting for markdown files

Where do I get the latest version?
https://github.com/dodmi/NPP/tree/master/userDefinedLangs

When was this file updated?
2020-07-26

-->
<NotepadPlus>
    <UserLang name="Markdown" ext="md markdown" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="no" foldCompact="no" forcePureLC="2" decimalSeparator="0" />
            <Prefix Keywords1="yes" Keywords2="yes" Keywords3="yes" Keywords4="yes" Keywords5="yes" Keywords6="yes" Keywords7="yes" Keywords8="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">00# 01 02((EOL)) 03&lt;!-- 04--&gt;</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1">.</Keywords>
            <Keywords name="Numbers, suffix2">.</Keywords>
            <Keywords name="Numbers, range"></Keywords>
            <Keywords name="Operators1">@ &lt; &gt; \&lt; \&gt; \\ \` \* \_ \{ \} \[ \] \( \) \# \+ \- \. \! \~ :|: |: :| | \|</Keywords>
            <Keywords name="Operators2">- +</Keywords>
            <Keywords name="Folders in code1, open"></Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close"></Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            <Keywords name="Keywords1">http:// (http:// https:// (https:// mailto: (mailto: ftp:// (ftp:// ftps:// (ftps:// (/ /</Keywords>
            <Keywords name="Keywords2">==== ----</Keywords>
            <Keywords name="Keywords3">*** ___</Keywords>
            <Keywords name="Keywords4">** __</Keywords>
            <Keywords name="Keywords5">* _</Keywords>
            <Keywords name="Keywords6">** __</Keywords>
            <Keywords name="Keywords7">* _</Keywords>
            <Keywords name="Keywords8"></Keywords>
            <Keywords name="Delimiters">00![ 00[ 01\ 02] 02] 03``` 03` 03~~~ 04\ 05``` 05((EOL `)) 05~~~ 06*** 07\ 08((EOL ***)) 09** 10\ 11((EOL **)) 12* 13\ 14((EOL *)) 15** 16\ 17((EOL **)) 18* 19\ 20((EOL *)) 21 22 23</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" fgColor="000001" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" fgColor="93A1A1" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="2" nesting="0" />
            <WordsStyle name="LINE COMMENTS" fgColor="268BD2" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="NUMBERS" fgColor="6C71C4" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="2AA198" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="268BD2" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="2" nesting="0" />
            <WordsStyle name="KEYWORDS6" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS7" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS8" fgColor="000001" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="OPERATORS" fgColor="D33682" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="FOLDER IN CODE1" fgColor="000001" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2" fgColor="000001" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="000001" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="2AA198" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="2" nesting="0" />
            <WordsStyle name="DELIMITERS2" fgColor="859900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS3" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="DELIMITERS4" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="1" nesting="65600" />
            <WordsStyle name="DELIMITERS5" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="2" nesting="32800" />
            <WordsStyle name="DELIMITERS6" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="DELIMITERS7" fgColor="B58900" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="DELIMITERS8" fgColor="000001" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
